{% block javascripts %}
{% do assets.addInlineJs("
    var timeline_div_height = $('#timeline-education').height();
    var last_div_top = $('#timeline-education .timeline-block:last-child').height();
    
    var line_height = timeline_div_height - last_div_top;

    $('#timeline-education').append('<style>#timeline-education::before{height: '+line_height+'px}</style>')
    ", 86) %}
{% endblock %}