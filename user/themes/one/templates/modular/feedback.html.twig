                    {% if item.use_default_icon == 0 %}
                         
                        {% if item.use_custom_icon == 1 and item.custom_icon %}    
                            <i class="fa fa-{{ item.custom_icon }}"></i> 
                        {% elseif item.letter %}
                            <h6>{{ item.letter }}</h6>
                        {% endif %}
                    {% else %}
                        <i class="fa fa-commenting"></i>
